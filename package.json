{
  "name": "svelte-icon-generator",
  "version": "0.9.0",
  "description": "Auto generate Svelte icon components.",
  "bin": {
    "svig": "./dist/index.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc && cp -r src/data src/templates dist",
    "start": "ts-node src/index.ts"
  },
  "keywords": [
    "svelte",
    "icons",
    "typescript",
    "interface",
    "cli",
    "generator"
  ],
  "author": "Ankur Seth",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.15.2"
  },
  "dependencies": {
    "handlebars": "^4.7.8"
  }
}
